    "@types/ffi-napi": "^4.0.10",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "electron": "^20.3.8",
    "electron-builder": "^25.0.5",
    "electron-rebuild": "^3.2.9",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.1",
    "nodemon": "^3.1.7",
    "style-loader": "^4.0.0",
